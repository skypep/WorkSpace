<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/toro_search_box_left_padding"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/toro_phone_number_text"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/toro_margin_right"
        android:layout_marginRight="@dimen/toro_margin_right"
        android:layout_marginTop="@dimen/toro_margin_top"
        android:textSize="@dimen/toro_call_details_action_item"
        android:textColor="#000000"
        android:layout_marginBottom="@dimen/toro_margin_top"
        />

</LinearLayout>

