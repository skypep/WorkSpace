<?xml version="1.0" encoding="utf-8"?>

<!--
android:background="#800000ff"
-->
<com.android.camera.module.MyColorEffectTopLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/color_effect_top_container_1"
    android:background="#ff000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--
    android:layout_centerVertical="true"
    -->
    <LinearLayout
        android:id="@+id/color_effect_top_container_2"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_marginTop="8dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_1"

                android:layout_marginLeft="8dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_2"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_3"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="8dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="8dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_4"

                android:layout_marginLeft="8dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_5"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_6"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="8dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="8dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_7"

                android:layout_marginLeft="8dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_8"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="4dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
            <com.android.camera.module.MyTextureView2
                android:id="@+id/color_effect_9"

                android:layout_marginLeft="4dip"
                android:layout_marginRight="8dip"
                android:layout_width="0dip"
                android:layout_height="1dip"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/exit_button_01"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="50dip"
        android:layout_marginBottom="60dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/chus_ic_filter_grid_button_back"/>

    <Button
        android:id="@+id/change_group_button_01"
        android:text="change_group"
        android:layout_above="@id/exit_button_01"
        android:layout_alignParentRight="true"
        android:layout_marginRight="50dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
</com.android.camera.module.MyColorEffectTopLinearLayout>